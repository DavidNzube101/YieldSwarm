# RPC Endpoints
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/YOUR_INFURA_PROJECT_ID
BSC_RPC_URL=https://bsc-dataseed1.binance.org/
POLYGON_RPC_URL=https://polygon-rpc.com/
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# API Keys
ETHERSCAN_API_KEY=YOUR_ETHERSCAN_API_KEY  # Works for all chains (Ethereum, BSC, Polygon, Arbitrum, etc.)
ALCHEMY_API_KEY=YOUR_ALCHEMY_API_KEY      # For Ethereum and Polygon
INFURA_PROJECT_ID=YOUR_INFURA_PROJECT_ID  # For Ethereum

# Wallet Configuration
WALLET_PRIVATE_KEY=YOUR_PRIVATE_KEY_HERE  # Your wallet private key (keep secure!)
WALLET_ADDRESS=YOUR_WALLET_ADDRESS_HERE   # Your wallet address for monitoring

# Optional: Additional RPC providers for redundancy
BSC_RPC_URL_BACKUP=https://bsc-dataseed2.binance.org/
POLYGON_RPC_URL_BACKUP=https://polygon-rpc.com/
ARBITRUM_RPC_URL_BACKUP=https://arb1.arbitrum.io/rpc

# The Graph endpoints (no API keys needed)
THEGRAPH_ENDPOINT=https://api.thegraph.com/subgraphs/name/
UNISWAP_V2_SUBGRAPH=uniswap/uniswap-v2
UNISWAP_V3_SUBGRAPH=uniswap/uniswap-v3
SUSHISWAP_SUBGRAPH=sushiswap/exchange

# PancakeSwap subgraphs
PANCAKESWAP_V2_BSC=https://api.thegraph.com/subgraphs/name/pancakeswap/exchange-v2
PANCAKESWAP_V3_BSC=https://api.thegraph.com/subgraphs/name/pancakeswap/exchange-v3
PANCAKESWAP_V2_ETH=https://api.thegraph.com/subgraphs/name/pancakeswap/exchange-v2-eth
PANCAKESWAP_V3_ETH=https://api.thegraph.com/subgraphs/name/pancakeswap/exchange-v3-eth

# Raydium v3 API endpoint
RAYDIUM_V3_API=https://api-v3.raydium.io/

# Risk Management
MAX_SLIPPAGE=0.5
GAS_LIMIT_MULTIPLIER=1.2
MIN_LIQUIDITY_THRESHOLD=10000
MAX_PORTFOLIO_RISK=0.3

# Logging
LOG_LEVEL=info
LOG_TO_FILE=true
LOG_FILE_PATH=./logs/yieldswarm.log

# Monitoring
HEALTH_CHECK_INTERVAL=30000
RISK_MONITORING_INTERVAL=60000
PORTFOLIO_UPDATE_INTERVAL=120000 

# Use mock DEX data (true = mock, false = real DEX API data)
USE_MOCK_DEX_DATA=false 